{
  "name": "vite-plugin-markdown-with-prismjs",
  "version": "1.0.2",
  "description": "Import markdown files in vite with customized classes and Prismjs processing",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc -w -p .",
    "build": "tsc -p .",
    "lint": "eslint . --ext .ts",
    "test:acceptance": "npm run cypress:run",
    "cypress:run": "cypress run --config-file test/acceptance/cypress.json",
    "cypress:open": "cypress open --config-file test/acceptance/cypress.json"
  },
  "keywords": [
    "vite",
    "frontmatter",
    "markdown",
    "react",
    "vue"
  ],
  "author": "VaJoy Lan <vajoy@qq.com>",
  "contributors": [
    "Kengo Hamasaki <k.hamasaki@gmail.com>",
    "sapphi-red <green@sapphi.red>",
    "Alex Zheng <zhibo90@gmail.com>",
    "Dafrok <o.o@mug.dog>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/VaJoy/vite-plugin-markdown-with-prismjs.git"
  },
  "bugs": {
    "url": "https://github.com/VaJoy/vite-plugin-markdown-with-prismjs/issues"
  },
  "homepage": "https://github.com/VaJoy/vite-plugin-markdown-with-prismjs/tree/main/#readme",
  "dependencies": {
    "front-matter": "^4.0.0",
    "he": "^1.2.0",
    "htmlparser2": "^6.0.0",
    "markdown-it": "^12.0.0",
    "prismjs": "^1.27.0"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@types/he": "^1.1.2",
    "@types/markdown-it": "12.0.1",
    "@types/node": "^16.0.0",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vue/compiler-sfc": "3.2.26",
    "cypress": "9.2.0",
    "eslint": "7.22.0",
    "typescript": "4.5.4",
    "vite": "2.4.0"
  }
